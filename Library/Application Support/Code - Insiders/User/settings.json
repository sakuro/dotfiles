{
    "clock.dateFormat": "yyyy-mm-dd (ddd) HH:MM:ss",
    "editor.copyWithSyntaxHighlighting": false,
    "editor.fontFamily": "Cica",
    "editor.fontSize": 16,
    "editor.insertSpaces": true,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "boundary",
    "editor.trimAutoWhitespace": true,
    "editor.wrappingIndent": "indent",
    "explorer.confirmDelete": false,
    "explorer.sortOrder": "mixed",
    "extensions.autoUpdate": true,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "files.associations": {
        ".irbrc": "ruby",
        ".pryrc": "ruby",
        ".gemrc": "yaml",
        ".direnvrc": "shellscript",
        ".zplug-packages": "shellscript",
        ".zunit": "shellscript",
        "*.zunit": "shellscript"
    },
    "files.exclude": {
        ".bundle": true,
        ".idea": true,
        ".vscode": true,
        ".yardoc": true,
        "db/*.sqlite3": true,
        "doc/": true,
        "log/": true,
        "pkg/": true,
        "tmp/": true,
        "vendor/": true
    },
    "files.hotExit": "off",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.confirmSync": false,
    "rust-client.disableRustup": true,
    "solargraph.commandPath": "/Users/sakuro/.asdf/shims/solargraph",
    "solargraph.bundlerPath": "/Users/sakuro/.asdf/shims/bundle",
    "terminal.integrated.fontSize": 18,
    "terminal.integrated.rendererType": "dom",
    "workbench.colorTheme": "Nord",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.startupEditor": "newUntitledFile",
    "[yaml]": {
      "editor.trimAutoWhitespace": false,
      "files.trimFinalNewlines": false,
      "files.trimTrailingWhitespace": false
    }
}
