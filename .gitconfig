[user]
	name = OZAWA Sakuro
	email = sakuro@2238club.org
[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
[alias]
	sw = checkout
	st = status -s
	cdiff = diff --word-diff=color --word-diff-regex=.
	egrep = grep -E
	ci = commit -v
	unstage = rm --cached
	l = log --stat --pretty=format:'commit %C(green)%H%Creset%nAuthor: %C(blue)%an %x3c%ae%x3e%Creset%nDate:   %aD%n%n%C(cyan)    %s%Creset%n%n%w(0,4,4)%b'
[log]
	date = local
[push]
	default = tracking
[pull]
	rebase = true
[branch]
	autosetuprebase = always
