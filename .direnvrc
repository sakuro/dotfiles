function use_postgresql {
  export PGHOST=localhost
  export PGUSER=postgres
}

if [ -f Gemfile.lock ] || [ -f gems.locked ]; then
  for bin in vendor/bundle/bin bin; do
    [ -d $bin ] && PATH_add $bin || :
  done
fi
[ -d node_modules/.bin ] && PATH_add node_modules/.bin || :

[ -d .git ] && git status -sb || :

